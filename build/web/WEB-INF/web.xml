<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>carpeta.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>carpeta.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DPServlet</servlet-name>
        <servlet-class>carpeta.servlets.DPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DDServlet</servlet-name>
        <servlet-class>carpeta.servlets.DDServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PackageDiagramServlet</servlet-name>
        <servlet-class>carpeta.servlets.PackageDiagramServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerDiagramaServlet</servlet-name>
        <servlet-class>carpeta.servlets.VerDiagramaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarDiagramaServlet</servlet-name>
        <servlet-class>carpeta.servlets.EliminarDiagramaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveServlet</servlet-name>
        <servlet-class>carpeta.servlets.SaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DPServlet</servlet-name>
        <url-pattern>/DPServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DDServlet</servlet-name>
        <url-pattern>/DDServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PackageDiagramServlet</servlet-name>
        <url-pattern>/PackageDiagramServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerDiagramaServlet</servlet-name>
        <url-pattern>/VerDiagramaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarDiagramaServlet</servlet-name>
        <url-pattern>/EliminarDiagramaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveServlet</servlet-name>
        <url-pattern>/SaveServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
